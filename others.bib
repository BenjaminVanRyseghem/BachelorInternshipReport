  publisher = {Springer-Verlag},
  series = {LNCS},
  title = {Attaching Second-Order Types to Methods in an
                  Object-Oriented Language},
  url = {http://link.springer.de/link/service/series/0558/tocs/t0707.htm},
  volume = {707},
